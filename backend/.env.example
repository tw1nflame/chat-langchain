# Example .env for local development

# Database connection (SQLAlchemy URL) - Postgres for local development
# Using Postgres on localhost:5432 with user admin / password admin and database n8n
DATABASE_URL=postgresql://postgres.your-tenant-id:your-super-secret-and-long-postgres-password@localhost:5433/postgres


# Comma-separated list of allowed CORS origins
CORS_ORIGINS=http://localhost:3000,http://localhost:3002,http://localhost:5173,http://localhost:4173

# Webhook URL the server will call (optional)
AGENT_DATABASE_URL=postgresql://postgres.your-tenant-id:your-super-secret-and-long-postgres-password@localhost:5433/postgres
# Supabase (optional) - set this if you use Supabase auth on frontend
